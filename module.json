{
  "id": "foundry-wled",
  "title": "WLED Integration",
  "description": "This module integrates Foundry VTT with WLED for controlling real world LEDs",
  "version": "#{VERSION}#",
  "library": "false",
  "manifestPlusVersion": "1.2.0",
  "compatibility": {
    "minimum": 12,
    "verified": 12,
    "maximum": 12
  },
  "authors": [
    {
      "name": "Justin Conabree",
      "url": "https://github.com/jconabree"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/refs/tags/release-4.1.1/system.json",
        "compatibility": {
          "verified": "4.1.1"
        }
      }
    ],
    "requires": []
  },
  "esmodules": [
    "scripts/module.js"
  ],
  "scripts": [
    "scripts/lib/lib.js"
  ],
  "styles": [
    "styles/module.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "languages/en.json"
    }
  ],
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "media": [
    {
      "type": "icon",
      "url": "https://raw.githubusercontent.com/jconabree/foundry-wled/refs/heads/main/icons/logo.png"
    }
  ]
}
